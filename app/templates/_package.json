{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "keywords": [
    <%- keywords.map(word => JSON.stringify(word)).join(',\n ') %>
  ],
  "scripts": {
    "clean": "rm -rf dist; rm -rf demo",
    "build-dist": "microbundle",
    "buidl-demo": "mkdir demo; browserify ./src/demo.ts -p [ tsify ] | uglifyjs -cm > demo/index.js; cp -r public/* demo/",
    "build": "npm run clean; npm run build-dist; npm run buidl-demo; rm src/index.js; ",
    "start": "budo ./src/demo.ts:index.js --open --live --dir public -- -p [ tsify ]",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "source": "src/index.ts",
  "main": "dist/<%= moduleName %>.js",
  "umd:main": "dist/<%= moduleName %>.umd.js",
  "module": "dist/<%= moduleName %>.m.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^16.2.2",
    "budo": "^11.3.2",
    "canvas-fit-margin-ts": "^1.0.8",
    "microbundle": "^0.6.0",
    "tsify": "^4.0.0",
    "typescript": "^3.0.1"
  }
}
